package ru.masterdm.km.web.model.services.util;

import java.io.InputStream;

import ru.masterdm.km.web.model.services.AttachmentStreamResponse;

public class JPEGAttachment extends AttachmentStreamResponse {
	public JPEGAttachment(InputStream is, String args) {
		super(is, args);
		this.contentType = "image/jpeg";
		this.extension = "jpg";
	}

	public JPEGAttachment(InputStream is) {
		super(is);
		this.contentType = "image/jpeg";
		this.extension = "jpg";
	}
}

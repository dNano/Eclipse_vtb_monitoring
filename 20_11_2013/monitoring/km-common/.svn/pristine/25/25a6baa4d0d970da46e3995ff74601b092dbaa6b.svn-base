package ru.masterdm.km.common.entity;

import java.util.List;

/**
 * Тип Условия Instance.
 * 
 * @author Shafigullin Ildar
 * 
 */
@SuppressWarnings("serial")
public class ConditionTypeInstance extends BaseNamedEntity<Long> {
	private ConditionType type;
	private List<DealCondition> conditions;

	public List<DealCondition> getConditions() {
		return conditions;
	}

	public void setConditions(List<DealCondition> conditions) {
		this.conditions = conditions;
	}

	public ConditionType getType() {
		return type;
	}

	public void setType(ConditionType type) {
		this.type = type;
	}
}
